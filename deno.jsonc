{
    "imports": {
        "@std/fs": "jsr:@std/fs@0.229.3",

        "@oak/acorn": "jsr:@oak/acorn@0.7.1",
        // mcnbt probably shouldn't be used, however everything else is slow af
        "mcnbt": "npm:mcnbt@2.0.3",
    },

    "tasks": {
        "run": "deno run --allow-net --allow-read ./src/main.ts",
        "dev": "deno run --allow-net --allow-read --watch src/main.ts",

        "fmt": "deno run --allow-sys --allow-env --allow-read --allow-write npm:prettier ./ --write",
    },
}
